{
    "version": 2,
    "framework": null,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      { "src": "/(css|js|build|assets|images|fonts)/(.*)", "dest": "/public/$1/$2" },
      { "src": "/(favicon\\.ico|robots\\.txt|site\\.webmanifest)", "dest": "/public/$1" },
      { "src": "/(.*)", "dest": "/api/index.php" }
    ]
}